    {
      "description": "Spotify Controls",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "equal_sign",
            "modifiers": {
              "mandatory": ["left_command"]
            }
          },
          "to": [
            {
              "shell_command": "/Users/santos/.local/bin/spotify-volume-up"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "hyphen",
            "modifiers": {
              "mandatory": ["left_command"]
            }
          },
          "to": [
            {
              "shell_command": "/Users/santos/.local/bin/spotify-volume-down"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "slash",
            "modifiers": {
              "mandatory": ["left_command"]
            }
          },
          "to": [
            {
              "shell_command": "/Users/santos/.local/bin/spotify-play-pause"
            }
          ]
        }
      ]
    },
