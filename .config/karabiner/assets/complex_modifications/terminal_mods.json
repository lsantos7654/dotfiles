    {
      "description": "caps_lock is right command",
      "manipulators": [
        {
          "type": "basic",
          "conditions": [
            {
              "bundle_identifiers": [
                "^org\\.mozilla\\.firefox",
                "^com\\.microsoft\\.edgemac",
                "^com\\.apple\\.Safari",
                "^com\\.google\\.Chrome",
                "^org\\.chromium\\.Chromium",
                ".*firefoxpwa.*"
              ],
              "type": "frontmost_application_if"
            }
          ],
          "from": {
            "key_code": "left_control",
            "modifiers": {
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "right_command"
            }
          ]
        }
      ]
    },
