    {
      "description": "Yabai controls",
      "manipulators": [
        {
          "description": "Close current window",
          "type": "basic",
          "from": {
            "key_code": "q",
            "modifiers": {
              "mandatory": ["left_command"]
            }
          },
          "to": [
            {
              "shell_command": "/opt/homebrew/bin/yabai -m window --close"
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
								"^com.apple.finder",
								"^com.apple.Preview",
                "^org\\.mozilla\\.firefox$",
                ".*firefoxpwa.*"
							]
            }
          ]
        },
